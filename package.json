{
  "name": "pede-ai",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/http/server.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/http/server.ts",
    "start": "./server",
    "check-types": "tsc --noEmit",
    "check": "biome check --write .",
    "pretest": "bun --env-file=.env.test drizzle-kit migrate",
    "test": "bun --env-file=.env.test test",
    "test:watch": "bun --env-file=.env.test test --watch",
    "test:coverage": "bun --env-file=.env.test test --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/eden": "^1.3.2",
    "@elysiajs/opentelemetry": "^1.3.1",
    "@elysiajs/swagger": "^1.3.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/instrumentation-pg": "^0.55.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.44.4",
    "drizzle-seed": "^0.3.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "latest",
    "pg": "^8.16.3",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@tsconfig/bun": "^1.0.8",
    "@types/pg": "^8.15.5",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4"
  },
  "module": "src/http/server.ts"
}
